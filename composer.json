{
  "name": "saeven/zf3-twig-trans",
  "description": "ZF3 module that glues Twig's {% trans %} and the standard translator together",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Saeven/zf3-twig-trans",
  "keywords": [
    "zf2",
    "twig",
    "trans",
    "translator",
    "i18n"
  ],
  "authors": [
    {
      "name": "Alexandre Lemaire",
      "email": "alemaire@circlical.com",
      "homepage": "http://circlical.com/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "~8.0.12 || ~8.1.0 || ~8.2.0",
    "ext-gettext": "*",
    "kokspflanze/zfc-twig": "^4.1.1",
    "twig/twig": "^3.0.1",
    "symfony/twig-bridge": "@stable",
    "laminas/laminas-servicemanager": "@stable",
    "laminas/laminas-mvc-i18n": "@stable",
    "laminas/laminas-mvc": "@stable",
    "laminas/laminas-view": "@stable",
    "laminas/laminas-coding-standard": "^2.5.0",
    "psr/container": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.27",
    "codacy/coverage": "^1.0"
  },
  "autoload": {
    "psr-0": {
      "CirclicalTwigTrans\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "cs": "mkdir -p tmp && vendor/bin/phpcs"
  }
}
